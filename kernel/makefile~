GCC=gcc -I ../include/ -c -o
NASM=nasm -f elf -o
kernel:
	$(GCC) ../myos.o myos.c
	$(GCC) ../task.o task.c
	$(GCC) ../memory.o memory.c
	$(GCC) ../string.o string.c
	$(GCC) ../signal.o signal.c
	cd arch_x86 && make arch_x86
	cd IPC && make message
	cd process_manage && make pm
	cd memory_manage && make mm
	cd init && make init